@model List<PhotoContest.App.Models.Contest.PrizeViewModel>

@{
    ViewBag.Title = "Prizes";
}

<div class="col-md-7">
    <h2>Prizes</h2>
    @for (int i = 0; i < Model.Count; i++)
    {
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Prize #@(i + 1). @Model[i].Name</h3>
            </div>
            <div class="panel-body">
                @Model[i].Description
            </div>
        </div>
    }
</div>